<h1><%= @organization.org_name %></h1>
<div>
<ul>
    <% @organization.attributes.each do |key, value| %>
    <% next if ["org_pwd"].include? key %>
    <li><strong><%= key.capitalize %>:</strong> <%= value %></li>
    <% end %>
    
</ul>

  <%= link_to "Edit this organization", edit_admin_organization_path(@organization) %> |
  <%= link_to "Back to organizations", admin_organizations_path %>

  <%= button_to "Destroy this organization", [:admin, @organization], method: :delete %>
</div>